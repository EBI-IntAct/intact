<class-descriptor
    class="uk.ac.ebi.intact.model.Protein"
    table="ia_Interactor"
>
      <!-- ***************** fields from superclasses ********************* -->
      <!-- fields from BasicObject... -->
      <field-descriptor id="1"
         name="ac"
         column="ac"
         jdbc-type="VARCHAR"
         primarykey="true" autoincrement="true"
      />
      <field-descriptor id="2"
         name="ownerAc"
         column="owner_ac"
         jdbc-type="VARCHAR"
      />
      <field-descriptor id="3"
         name="created"
         column="created"
         jdbc-type="TIMESTAMP"
         nullable="false"
      />
      <field-descriptor id="4"
         name="updated"
         column="updated"
         jdbc-type="TIMESTAMP"
         nullable="false"
      />
      <field-descriptor id="5"
         name="shortLabel"
         column="shortLabel"
         jdbc-type="VARCHAR"
         indexed="true"
      />
      <field-descriptor id="6"
         name="fullName"
         column="fullName"
         jdbc-type="VARCHAR"
         indexed="true"
      />
      <field-descriptor id="7"
         name="ojbConcreteClass"
         column="objClass"
         jdbc-type="VARCHAR"
      />
      <field-descriptor id="8"
         name="bioSourceAc"
         column="bioSource_ac"
         jdbc-type="VARCHAR"
         indexed="true"
      />
      <field-descriptor id="10"
         name="crc64"
         column="crc64"
         jdbc-type="VARCHAR"
         indexed="true"
      />
      <field-descriptor id="11"
         name="formOfAc"
         column="formOf"
         jdbc-type="VARCHAR"
      />


      <!-- reference classes -->
      <!-- There is a column for a proteinForm field, which should be a
      CvProteinForm, but this is to be left out for now (not intactCore).. -->
      <!--  The self-reference seems to cause severe problems for OJB. Left out for now.
      <reference-descriptor
         name="formOf"
         class-ref="uk.ac.ebi.intact.model.Protein"
      >
         <foreignkey field-id-ref="12"/>
      </reference-descriptor>

      -->

      <reference-descriptor
         name="owner"
         class-ref="uk.ac.ebi.intact.model.Institution"
      >
         <foreignkey field-id-ref="2"/>
      </reference-descriptor>


      <reference-descriptor
         name="bioSource"
         class-ref="uk.ac.ebi.intact.model.BioSource"
      >
         <!-- key from a BioSource to an Interactor -->
         <foreignkey field-id-ref="8"/>
      </reference-descriptor>

       <!-- fields from AnnotatedObject (intactCore only) -->
     <collection-descriptor
         name="xref"
         proxy="true"
         element-class-ref="uk.ac.ebi.intact.model.Xref"
     >

         <inverse-foreignkey field-id-ref="10"/>
      </collection-descriptor>

       <!-- Interactors and Annotations are linked via a (non-qualified) indirection
            table INT2ANNOT..... -->
     <collection-descriptor
         name="annotation"
         element-class-ref="uk.ac.ebi.intact.model.Annotation"
         proxy="true"
         indirection-table="ia_INT2ANNOT"

      >
         <fk-pointing-to-this-class column="interactor_ac"/>
         <fk-pointing-to-element-class column="annotation_ac"/>
      </collection-descriptor>

     <collection-descriptor
         name="activeInstance"
         element-class-ref="uk.ac.ebi.intact.model.Component"
	 proxy="true"
      >
        <inverse-foreignkey field-id-ref="6"/>
      </collection-descriptor>

   <collection-descriptor
       name="sequenceChunks"
       proxy="true"
       element-class-ref="uk.ac.ebi.intact.model.SequenceChunk"
       orderby="sequence_index"
       sort="ASC"
   >

       <inverse-foreignkey field-id-ref="4"/>
    </collection-descriptor>


   </class-descriptor>




