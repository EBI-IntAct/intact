<project>
    <build>
        <plugins>
            <plugin>
                <artifactId>uniprotexport-maven-plugin</artifactId>
                <configuration>
                    <targetPath>target/uniprotexport-test</targetPath>
                    <uniprotLinksFilename>uniprotlinks.dat</uniprotLinksFilename>
                    <summaryFile>target/uniprotexport-test/line_exported_summary.log</summaryFile>
                    <maxProteinsToExport>2</maxProteinsToExport>
                    <overwrite>true</overwrite>
                    <hibernateConfig>target/hibernate/config/hibernate.cfg.xml</hibernateConfig>
                </configuration>
            </plugin>
        </plugins>
     </build>
</project>