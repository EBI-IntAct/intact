<plugin>
  <description>Plugin for IntAct</description>
  <groupId>uk.ac.ebi.intact.plugins</groupId>
  <artifactId>update-db-proteins-maven-plugin</artifactId>
  <version>1.0-SNAPSHOT</version>
  <goalPrefix>update-db-proteins</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>update-proteins</goal>
      <description>Example mojo. This mojo is executed when the goal &quot;mygoal&quot; is called.
Change this comments and the goal name accordingly</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>process-resources</phase>
      <implementation>uk.ac.ebi.intact.plugins.updateDbProteins.UpdateDbProteinsMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>dummyFile</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>An example file</description>
        </parameter>
        <parameter>
          <name>hibernateConfig</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>Project instance</description>
        </parameter>
      </parameters>
      <configuration>
        <hibernateConfig implementation="java.io.File">${project.build.directory}/hibernate/config/hibernate.cfg.xml</hibernateConfig>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <dummyFile implementation="java.io.File" default-value="${project.build.directory}/dummy.txt"/>
      </configuration>
    </mojo>
  </mojos>
  <dependencies/>
</plugin>