<project>
    <build>
        <plugins>
            <plugin>
                <artifactId>uniprotexport-maven-plugin</artifactId>

                <configuration>
                    <indexPath>target/index</indexPath>
                    <uniprotLinksFilename>target/uniprotlinks.dat</uniprotLinksFilename>
                    <updatedUniprotLinksFilename>target/uniprotlinks.updated.dat</updatedUniprotLinksFilename>
                    <overwrite>true</overwrite>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>