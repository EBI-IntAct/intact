<project>
    <build>
        <plugins>
            <plugin>
                <artifactId>dbunit-dataset-maven-plugin</artifactId>
                <configuration>
                    <project implementation="uk.ac.ebi.intact.unitdataset.plugin.StubProject"/>
                    <providerName>PsiTestDatasetProvider</providerName>
                    <cvConfiguration>
                        <oboUrl>http://intact.svn.sourceforge.net/viewvc/*checkout*/intact/repo/utils/data/controlledVocab/psi-mi25-4intact.obo?revision=8645</oboUrl>
                        <additionalUrl>http://intact.svn.sourceforge.net/viewvc/*checkout*/intact/repo/utils/data/controlledVocab/intact-unique.csv</additionalUrl>
                        <additionalAnnotationsUrl>http://intact.svn.sourceforge.net/viewvc/*checkout*/intact/repo/utils/data/controlledVocab/additional-annotations.csv</additionalAnnotationsUrl>
                    </cvConfiguration>
                    <datasets>
                        <dataset>
                            <id>ALL_CVS</id>
                            <containsAllCVs>true</containsAllCVs>
                        </dataset>
                        <dataset>
                            <id>INTACT_JUL_06</id>
                            <files>
                                <file>src/test/resources/xml/intact_2006-07-19.xml</file>
                            </files>
                        </dataset>
                    </datasets>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>
