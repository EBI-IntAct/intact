<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <parent>
        <groupId>uk.ac.ebi.intact.dataexchange.psimi</groupId>
        <artifactId>intact-psixml</artifactId>
        <version>2.0.0-SNAPSHOT</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>

    <artifactId>intact-psixml-upload-extensions</artifactId>
    <packaging>jar</packaging>

    <name>IntAct Data Exchange :: PSI MI :: XML :: PSI Extensions</name>
    <description>Uploads PSIXML files to the database</description>
    <url>http://www.ebi.ac.uk/~intact/devsite/intact-psixml-exchange</url>

    <scm>
        <connection>
            scm:svn:https://intact.svn.sourceforge.net/svnroot/intact/repo/intact-psixml/intact-psixml-exchange/trunk
        </connection>
        <developerConnection>
            scm:svn:https://intact.svn.sourceforge.net/svnroot/intact/repo/intact-psixml/intact-psixml-exchange/trunk
        </developerConnection>
        <url>http://svn.sourceforge.net/viewcvs.cgi/intact/repo/intact-psixml/intact-psixml-exchange/trunk/</url>
    </scm>

    <dependencies>
        <dependency>
            <groupId>uk.ac.ebi.intact</groupId>
            <artifactId>intact-core</artifactId>
        </dependency>
        <dependency>
            <groupId>uk.ac.ebi.intact.dataexchange.psimi</groupId>
            <artifactId>intact-psixml-processor</artifactId>
        </dependency>
    </dependencies>

    <distributionManagement>
        <site>
            <id>intact-site</id>
            <name>Intact site</name>
            <url>${site.remote.basedir}/intact-psixml-upload-extensions</url>
        </site>
    </distributionManagement>

</project>
