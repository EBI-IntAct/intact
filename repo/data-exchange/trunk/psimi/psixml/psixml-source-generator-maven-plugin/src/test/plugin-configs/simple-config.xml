<project>
    <build>
        <plugins>
            <plugin>
                <artifactId>psixml-source-generator-maven-plugin</artifactId>
                <configuration>
                    <outputFile>target/myOutput.log</outputFile>
                    <targetPath>target/generated</targetPath>
                    <generatedPackage>uk.ac.ebi.intact.psixml.generated</generatedPackage>
                    <sourceGeneratorClass>uk.ac.ebi.intact.psixml.tools.generator.AnnotationSourceGenerator
                    </sourceGeneratorClass>
                    <project implementation="uk.ac.ebi.intact.dataexchange.psimi.xml.generator.StubProject"/>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>